<Window x:Class="StudentJSONSerialisation.View.CreateStudentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudentJSONSerialisation.View"
        mc:Ignorable="d"
        Title="CreateStudentWindow" Height="350" Width="300">
    <Grid>
        <TextBox x:Name="FirsNameBox" HorizontalAlignment="Center" Margin="0,50,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="LastNameBox" HorizontalAlignment="Center" Margin="0,100,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="AgeBox" HorizontalAlignment="Center" Margin="0,150,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button Style="{StaticResource AnotherWindowsButton}"  Content="Button" HorizontalAlignment="Center" Margin="0,230,0,0" VerticalAlignment="Top" Click="Button_Click"/>

    </Grid>
</Window>
