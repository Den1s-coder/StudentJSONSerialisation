<Window x:Class="StudentJSONSerialisation.View.DeleteStudentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudentJSONSerialisation.View"
        mc:Ignorable="d"
        Title="DeleteStudentWindow" Height="250" Width="200">
    <Grid>
        <ComboBox HorizontalAlignment="Center" SelectedItem="{Binding selectedStudent}" ItemsSource="{Binding Students}" Margin="0,60,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Style="{StaticResource AnotherWindowsButton}"  Content="Delete" HorizontalAlignment="Center" Margin="0,160,0,0" VerticalAlignment="Top" Click="DeleteButton"/>

    </Grid>
</Window>
